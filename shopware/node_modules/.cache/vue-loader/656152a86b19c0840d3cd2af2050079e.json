Card content-->\r\n    <div class=\"card-body px-lg-5 pt-0\">\r\n\r\n        <!-- Form -->\r\n      <form class=\"text-center\" style=\"color: #757575;\" action=\"#!\" @submit.prevent=\"loginUser\">\r\n        \r\n        <!-- First name -->\r\n        <div class=\"md-form mt-3\">\r\n          <input \r\n          type=\"text\" \r\n          id=\"materialRegisterFormFirstName\" \r\n          class=\"form-control\" \r\n          placeholder=\"Username\"\r\n          v-model=\"username\"\r\n          >\r\n        </div>\r\n\r\n          <!-- E-mail -->\r\n        <!-- <div class=\"md-form mt-3\">\r\n          <input \r\n          type=\"email\" \r\n          id=\"materialRegisterFormEmail\" \r\n          class=\"form-control\" \r\n          placeholder=\"Email\"\r\n          v-model=\"email\"\r\n          >\r\n        </div> -->\r\n\r\n          <!-- Password -->\r\n        <div class=\"md-form mt-3\">\r\n          <input \r\n          type=\"password\" \r\n          id=\"materialRegisterFormPassword\" \r\n          class=\"form-control\" \r\n          aria-describedby=\"materialRegisterFormPasswordHelpBlock\" \r\n          placeholder=\"Password\"\r\n          v-model=\"password\"\r\n          >\r\n          \r\n          <small id=\"materialRegisterFormPasswordHelpBlock\" class=\"form-text text-muted mb-4 mt-2\">\r\n            At least 8 characters\r\n          </small>\r\n        </div>\r\n\r\n          <!-- Sign up button -->\r\n        <input class=\"btn btn-outline-info btn-rounded btn-block my-4 waves-effect z-depth-0\" type=\"submit\" value=\"Login\">\r\n\r\n          <!-- Social register -->\r\n        <router-link to=\"/register\" class=\"card-link\"><span>Need an account?</span></router-link>\r\n\r\n        <!-- <a type=\"button\" class=\"btn-floating btn-fb btn-sm\">\r\n            <i class=\"fab fa-facebook-f\"></i>\r\n        </a>\r\n        <a type=\"button\" class=\"btn-floating btn-tw btn-sm\">\r\n            <i class=\"fab fa-twitter\"></i>\r\n        </a>\r\n        <a type=\"button\" class=\"btn-floating btn-li btn-sm\">\r\n            <i class=\"fab fa-linkedin-in\"></i>\r\n        </a>\r\n        <a type=\"button\" class=\"btn-floating btn-git btn-sm\">\r\n            <i class=\"fab fa-github\"></i>\r\n        </a> -->\r\n\r\n        <hr>\r\n\r\n          <!-- Terms of service -->\r\n        <p>By clicking\r\n          <em>Login</em> you agree to our\r\n          <a href=\"\" target=\"_blank\">terms of service</a>\r\n        </p>\r\n      </form>\r\n        <!-- Form -->\r\n    </div>\r\n  </div>\r\n<!-- Material form register -->\r\n</template>\r\n\r\n<script>\r\n\r\nimport { mapActions } from 'vuex';\r\n\r\nexport default {\r\n  data() {\r\n    return {\r\n      username: \"\",\r\n      password: \"\"\r\n    }\r\n  },\r\n  methods: {\r\n    ...mapActions(['login']),\r\n    loginUser() {\r\n      let user = {\r\n        username: this.username,\r\n        password: this.password\r\n      };\r\n      this.login(user)\r\n      .then(res => {\r\n        if (res.data.success) {\r\n          this.$router.push('/profile');\r\n        }\r\n      }).catch(err => {\r\n        console.log(err);\r\n      });\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n\r\n.card {\r\n  margin: auto;\r\n  width: 100%;\r\n  padding-top: 20px;\r\n}\r\n\r\na[target=_blank] {\r\n  color : rgba(23, 162, 191);\r\n}\r\n\r\n.card-link {\r\n  color : rgb(115, 171, 189);\r\n  transition: color 0.2s;\r\n}\r\n\r\n.card-link:hover {\r\n  color : rgba(23, 162, 191);\r\n}\r\n\r\ninput {\r\n  border-radius: 0;\r\n}\r\n\r\n.btn {\r\n  border-radius: 0;\r\n}\r\n\r\n</style>"]}]}