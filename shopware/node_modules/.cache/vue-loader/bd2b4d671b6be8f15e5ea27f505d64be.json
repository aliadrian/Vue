\n      <strong class=\"text-info\">Register</strong>\r\n    </h5>\r\n\r\n    <!--Card content-->\r\n    <div class=\"card-body px-lg-5 pt-0\">\r\n\r\n        <!-- Form -->\r\n      <form class=\"text-center\" style=\"color: #757575;\" action=\"#!\"  @submit.prevent=\"registerUser\">\r\n\r\n        <!-- Username -->\r\n        <div class=\"md-form mt-3\">\r\n          <input \r\n          type=\"text\" \r\n          id=\"username\" \r\n          class=\"form-control\" \r\n          placeholder=\"Username\"\r\n          name=\"username\"\r\n          v-model=\"username\"\r\n          >\r\n        </div>  \r\n        \r\n        <!-- Name -->\r\n        <div class=\"md-form mt-3\">\r\n          <input \r\n          type=\"text\" \r\n          id=\"name\" \r\n          class=\"form-control\" \r\n          placeholder=\"Name\"\r\n          name=\"name\"\r\n          v-model=\"name\"\r\n          >\r\n        </div>\r\n        \r\n          <!-- E-mail -->\r\n        <div class=\"md-form mt-3\">\r\n          <input \r\n          type=\"email\" \r\n          id=\"email\" \r\n          class=\"form-control\" \r\n          placeholder=\"Email\"\r\n          name=\"email\"\r\n          v-model=\"email\"\r\n          >\r\n        </div>\r\n\r\n          <!-- Password -->\r\n        <div class=\"md-form mt-3\">\r\n          <input \r\n          type=\"password\" \r\n          id=\"password\" \r\n          class=\"form-control\" \r\n          aria-describedby=\"materialRegisterFormPasswordHelpBlock\" \r\n          placeholder=\"Password\"\r\n          name=\"password\"\r\n          v-model.trim=\"$v.password.$model\"\r\n          :class=\"{ 'is-invalid': $v.password.$error,\r\n          'is-valid':!$v.password.$invalid}\">\r\n          <div class=\"valid-feedback\">Your password is valid.</div>\r\n          <div class=\"invalid-feedback\">\r\n            <span v-if=\"!$v.password.required\">Password is required.</span>\r\n            <span v-if=\"!$v.password.minLength\">{{ $v.password.$params.minLength.min }} characters minimum.</span>\r\n          </div>\r\n        <!-- </div> -->\r\n        \r\n        <div class=\"md-form mt-3\">\r\n          <input \r\n          type=\"password\" \r\n          id=\"materialRegisterFormPassword\" \r\n          class=\"form-control\"\r\n          aria-describedby=\"materialRegisterFormPasswordHelpBlock\" \r\n          placeholder=\"Confirm Password\"\r\n          v-model.trim=\"$v.confirm_password.$model\"\r\n          :class=\"{ \r\n            'is-invalid': $v.confirm_password.$error, \r\n            'is-valid': (password != '') ? !$v.confirm_password.$invalid : ''}\">\r\n            <div class=\"valid-feedback\">Password confirmed.</div>\r\n            <div class=\"invalid-feedback\">\r\n              <span v-if=\"!$v.confirm_password.sameAsPassword\">Passwords must be identical.</span>\r\n            </div>\r\n        </div>\r\n\r\n          <small id=\"materialRegisterFormPasswordHelpBlock\" class=\"form-text text-muted mb-4 mt-2\">\r\n            At least 8 characters\r\n          </small>\r\n        </div>\r\n\r\n          <!-- Sign up button -->\r\n        <button class=\"btn btn-outline-info btn-rounded btn-block my-4 waves-effect z-depth-0\">Register</button>\r\n\r\n          <!-- Social register -->\r\n\r\n        <router-link to=\"/login\" class=\"card-link\"><span>Already have an account?</span></router-link>\r\n\r\n        <!-- <p>or login with:</p> -->\r\n\r\n        <!-- <a type=\"button\" class=\"btn-floating btn-fb btn-sm\">\r\n            <i class=\"fab fa-facebook-f\"></i>\r\n        </a>\r\n        <a type=\"button\" class=\"btn-floating btn-tw btn-sm\">\r\n            <i class=\"fab fa-twitter\"></i>\r\n        </a>\r\n        <a type=\"button\" class=\"btn-floating btn-li btn-sm\">\r\n            <i class=\"fab fa-linkedin-in\"></i>\r\n        </a>\r\n        <a type=\"button\" class=\"btn-floating btn-git btn-sm\">\r\n            <i class=\"fab fa-github\"></i>\r\n        </a> -->\r\n\r\n        <hr>\r\n\r\n          <!-- Terms of service -->\r\n        <p>By clicking\r\n          <em>Login</em> you agree to our\r\n          <a href=\"\" target=\"_blank\">terms of service</a>\r\n        </p>\r\n      </form>\r\n        <!-- Form -->\r\n    </div>\r\n  </div>\r\n  \r\n<!-- Material form register -->\r\n</template>\r\n\r\n<script>\r\n\r\nimport { mapActions } from 'vuex';\r\n// import { ValidationProvider, extend, ValidationObserver } from \"vee-validate\";\r\nimport { required, minLength, sameAs } from 'vuelidate/lib/validators'\r\n// const alpha = helpers.regex('alpha', /^(?=.*[A-Za-z])(?=.*\\d)[A-Za-z\\d]{8,}$/)\r\n\r\nexport default {\r\n  components: {\r\n\r\n  },\r\n  data() {\r\n    return {\r\n      username: \"\",\r\n      email: \"\",\r\n      password: \"\",\r\n      name: \"\",\r\n      confirm_password: \"\"\r\n    };\r\n  },\r\n    validations: {\r\n    password: {\r\n      required,\r\n      minLength: minLength(8)\r\n    },\r\n    confirm_password: {\r\n      sameAsPassword: sameAs('password')\r\n    }\r\n  },\r\n  methods: {\r\n    ...mapActions([\"register\"]),\r\n    registerUser() {\r\n      let user = {\r\n        username: this.username,\r\n        password: this.password,\r\n        confirm_password: this.confirm_password,\r\n        email: this.email,\r\n        name: this.name\r\n      }\r\n      this.register(user).then(res => {\r\n        if (res.data.success) {\r\n          this.$router.push(\"login\");\r\n        }\r\n      });\r\n    }\r\n  }\r\n};\r\n\r\n</script>\r\n\r\n<style scoped>\r\n\r\n.card {\r\n  margin: auto;\r\n  width: 100%;\r\n  padding-top: 20px;\r\n}\r\n\r\na[target=_blank] {\r\n  color : rgba(23, 162, 191);\r\n}\r\n\r\n.card-link {\r\n  color : rgb(115, 171, 189);\r\n  transition: color 0.2s;\r\n}\r\n\r\n.card-link:hover {\r\n  color : rgba(23, 162, 191);\r\n}\r\n\r\ninput {\r\n  border-radius: 0;\r\n}\r\n\r\n.btn {\r\n  border-radius: 0;\r\n}\r\n\r\n\r\n</style>"]}]}